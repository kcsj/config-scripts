% cat puzzle-4.4 | ./finder2007_4/finder

sort {

     % int cardinality = 7.

     % animals enum: cat1, cat2, cat3, dog1, dog2, dog3.
     animals cardinality = 6.

     spots {
     	   cardinality < 8.
	   cardinality > 0.
     }
}

function {
	 occupied: animals -> spots {injective}.
	 % vacant: spots. {}

	 % move_left: animals -> spots {hidden}
	 % move_right: animals -> spots {hidden}

	 A: animals. {hidden} % skolem cat1
	 B: animals. {hidden} % skolem cat2
	 C: animals. {hidden} % skolem cat3
	 D: animals. {hidden} % skolem dog1
	 E: animals. {hidden} % skolem dog2
	 F: animals. {hidden} % skolem dog3

	 S1: spots. {hidden} % skolem spot1
	 S2: spots. {hidden} % skolem spot2
	 S3: spots. {hidden} % skolem spot3
	 S4: spots. {hidden} % skolem spot4
	 S5: spots. {hidden} % skolem spot5
	 S6: spots. {hidden} % skolem spot6
	 S7: spots. {hidden} % skolem spot7
}

clause {

       occupied(A) = S1.
       occupied(B) = S2.
       occupied(C) = S3.
       occupied(D) = S5.
       occupied(E) = S6.
       occupied(F) = S7.

       % occupied(cat1) = 0.
       % occupied(cat2) = 1.
       % occupied(cat3) = 2.
       % occupied(dog1) = 4.
       % occupied(dog2) = 5.
       % occupied(dog3) = 6.

}

% setting {
% 	ugly
% }
end
