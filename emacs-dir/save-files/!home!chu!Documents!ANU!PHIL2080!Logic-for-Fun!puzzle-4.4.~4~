% cat puzzle-4.4 | ./finder2007_4/finder

sort {

     int cardinality = 7.

     animals enum: cat1, cat2, cat3, dog1, dog2, dog3.

     spots {
     	   cardinality < 8.
	   cardinality > 0.
     }
}

function {
	 occupied: animals -> int {}
}

clause {

       occupied(cat1) = 0.
       occupied(cat2) = 1.
       occupied(cat3) = 2.
       occupied(dog1) = 4.
       occupied(dog2) = 5.
       occupied(dog3) = 6.

}

% setting {
% 	ugly
% }
end
