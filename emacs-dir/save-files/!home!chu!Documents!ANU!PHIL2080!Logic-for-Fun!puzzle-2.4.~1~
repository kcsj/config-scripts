#user#
chu

#sorts#
alien enum: vrrkth, wffmp, xq^rp, ydd, ykggh.
drink enum: supernova, black_hole, nebula, white_dwarf, warp_five.
planet enum: cc!txibb, edrrgh, bcqei, dph.
job enum: prospector, mercenary, gambler, robot_master, other.


#functions#
has_job: alien -> job {injective}.
is_drinking: alien -> drink {injective}.
is_from: alien -> planet {}.


#clauses#
% xq^rp is the robot master
has_job(xq^rp) = robot_master.

% ydd drank the supernova
is_drinking(ydd) = supernova.

% wffmp hails from neither dph nor edrrgh.
NOT (is_from(wffmp) = dph) AND NOT (is_from(wffmp) = edrrgh).



